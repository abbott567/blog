{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "nyc --reporter=html --reporter=text mocha"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "date-fns": "^1.28.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jquery": "^3.2.1",
    "marked": "^0.3.6",
    "morgan": "~1.6.1",
    "nunjucks": "^3.0.1",
    "nunjucks-markdown": "^2.0.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "ignores": [
      "public/**/*"
    ],
    "env": [
      "jquery",
      "browser",
      "mocha"
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
