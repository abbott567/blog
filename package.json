{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "nyc --reporter=html --reporter=text mocha"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "date-fns": "^1.28.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "marked": "^0.3.6",
    "morgan": "~1.6.1",
    "nunjucks": "^3.0.1",
    "nunjucks-markdown": "^2.0.1",
    "serve-favicon": "~2.3.0",
    "shrink-ray": "^0.1.3"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "pump": "^1.0.2",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "ignores": [
      "public/**/*"
    ],
    "overrides": [
      {
        "files": "src/assets/js/*.js",
        "esnext": false
      }
    ],
    "env": [
      "browser",
      "mocha"
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
